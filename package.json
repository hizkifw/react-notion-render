{
  "name": "@9gustin/react-notion-render",
  "version": "3.8.3",
  "description": "A library to render notion content",
  "author": "9gustin",
  "keywords": ["Notion", "Notion API", "React Notion", "Notion pages"],
  "license": "MIT",
  "repository": "9gustin/react-notion-render",
  "homepage": "https://github.com/9gustin/react-notion-render/",
  "bugs": "https://github.com/9gustin/react-notion-render/issues",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "dev": "npm-run-all --parallel start dev-example",
    "dev-example": "cd dev-example && npm run dev -- -p 3001",
    "prepare": "run-s build"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "cross-env": "^7.0.2",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.0",
    "typescript": "^4.3.2"
  },
  "files": ["dist"]
}
